{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "interface-name": [false, "always-prefix"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-unused-vars": false,
    "jsx-no-lambda": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "src/tests/*"
    ]
  }
}
